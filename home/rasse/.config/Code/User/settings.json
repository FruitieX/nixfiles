{
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "window.titleBarStyle": "custom",
  "editor.tabSize": 2,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback', 'Noto Color Emoji'",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "u"
      ],
      "after": [],
      "commands": [
        {
          "command": "undo",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<C-r>"
      ],
      "after": [],
      "commands": [
        {
          "command": "redo",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<C-]>"
      ],
      "after": [
        "y",
        "s",
        "w",
        "(",
        "w",
        "i",
        ":",
        ":",
        " ",
        "?",
        "x",
        "<Esc>"
      ],
    }
  ],
  "vim.handleKeys": {
    "<C-k>": false,
    "<C-d>": true
  },
  // try fixing vim extension performance
  "vim.useSystemClipboard": true,
  "vim.statusBarColorControl": false,
  "vim.easymotion": true,
  "vim.hlsearch": true,
  "vim.sneak": true,
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": true,
  "bracketPairColorizer.consecutivePairColors": [
    "()",
    "[]",
    "{}",
    [
      "<",
      "</"
    ],
    [
      "<",
      "/>"
    ],
    [
      "Gold",
      "Orchid",
      "LightSkyBlue"
    ],
    "Red"
  ],
  "window.zoomLevel": 0,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": true,
  "files.associations": {
    "*.json": "jsonc"
  },
  "keyboard.dispatch": "keyCode",
  "files.autoSave": "afterDelay",
  "workbench.editor.labelFormat": "medium",
  "prettier.requireConfig": true,
  "search.location": "sidebar",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.highlightModifiedTabs": true,
  "files.eol": "\n",
  "prettier.endOfLine": "lf",
  "editor.minimap.enabled": false,
  "jest.autoEnable": false,
  "jest.coverageFormatter": "GutterFormatter",
  "jest.showCoverageOnLoad": true,
  "jest.enableCodeLens": false,
  "editor.fontLigatures": true,
  "diffEditor.renderSideBySide": true,
  "explorer.confirmDragAndDrop": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "cSpell.userWords": [
    "comparator",
    "unmount",
    "autofetch",
    "monospace",
    "murmurhash",
    "rerender",
    "scrollbar"
  ],
  "vim.neovimPath": "/run/current-system/sw/bin/nvim",
  "vim.enableNeovim": true,
  "vsintellicode.modelDownloadPath": "/home/rasse/.config/Code/IntelliCode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.codeLens.enabled": false,
  "purescript.addSpagoSources": true,
  "workbench.editor.showTabs": false,
  "workbench.tree.indent": 16,
  "purescript.buildCommand": "spago build -- --json-errors -g sourcemaps",
  "purescript.editorMode": true,
  "search.useGlobalIgnoreFiles": true,
  "python.jediEnabled": false,
  "editor.renderWhitespace": "none",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "purescript",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "files.autoSaveDelay": 200,
  "editor.quickSuggestionsDelay": 200,
}